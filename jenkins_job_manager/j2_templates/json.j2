[{% for job_name, before_xml, after_xml, difflines, md, changetype in obj.job_changes %}
  {
    "name" : {{ job_name | tojson }},
    "before_xml":
      "{% for line in before_xml.splitlines() %}{{ line }}
      {% endfor %}",
    "after_xml":
      "{% for line in after_xml.splitlines() %}{{ line }}
      {% endfor %}",
    "diff":
      {% for line in difflines %}{{ line }}
      {% endfor %},
    "metadata": {{ md | tojson }},
    "change_type": {{ changetype | tojson }},
  },
{% endfor -%}]